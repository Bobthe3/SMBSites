{
  "name": "site-deployment",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "sites/*"
  ],
  "scripts": {
    "dev:keekuzcom": "npm run dev --workspace=sites/keekuzcom",
    "build:keekuzcom": "npm run build --workspace=sites/keekuzcom",
    "start:keekuzcom": "npm run start --workspace=sites/keekuzcom",
    "dev:template": "npm run dev --workspace=sites/template",
    "build:template": "npm run build --workspace=sites/template",
    "start:template": "npm run start --workspace=sites/template",
    "build:all": "npm run build --workspaces"
  },
  "dependencies": {
    "playwright": "^1.57.0"
  }
}
